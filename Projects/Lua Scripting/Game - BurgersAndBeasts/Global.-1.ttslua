function onObjectLeaveScriptingZone(zone, leave_object)
    local name = leave_object.getName()

    if zone == Pantry_Zone then
        if name == 'One' then
            player_one_pantry = false
        elseif name == 'Two' then
            player_two_pantry = false
        elseif name == 'Three' then
            player_three_pantry = false
        elseif name == 'Four' then
            player_four_pantry = false
        end
    end
end

function onObjectEnterScriptingZone(zone, enter_object)
    local name = enter_object.getName()

    if zone == Pantry_Zone then
        if name == 'One' then
            player_one_pantry = true
        elseif name == 'Two' then
            player_two_pantry = true
        elseif name == 'Three' then
            player_three_pantry = true
        elseif name == 'Four' then
            player_four_pantry = true
        end
    end


    if zone == Busser_Zone then
