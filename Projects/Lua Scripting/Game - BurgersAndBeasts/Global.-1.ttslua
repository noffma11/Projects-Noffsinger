function startingDeal (drawToThese)
    for i, v in ipairs(drawToThese) do
        if v == "One" then
            Starter_Toppings_One.dealToColor(5, "White")
        elseif v == "Two" then
            Starter_Toppings_Two.dealToColor(5, "Red")
        elseif v == "Three" then
            Starter_Toppings_Three.dealToColor(5, "Green")
        elseif v == "Four" then
            Starter_Toppings_Four.dealToColor(5, "Blue")
        end
    end
end

