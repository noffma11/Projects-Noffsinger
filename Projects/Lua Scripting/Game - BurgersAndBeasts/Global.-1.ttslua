function restOfDiscardDraw(dTable)
    local deckZone = dTable.cDraw.getObjects()
    local deck = deckZone[1]
    local playerHand
    local handSize
    local curPlayer = dTable.cPlayer

    if curPlayer == "Two" then
        playerHand = Player.Red.getHandObjects(1)
        handSize = Player_Two_Hand_Counter.getValue()
    elseif curPlayer == "One" then
        playerHand = Player.White.getHandObjects(1)
        handSize = Player_One_Hand_Counter.getValue()
    elseif curPlayer == "Three" then
        playerHand = Player.Green.getHandObjects(1)
        handSize = Player_Three_Hand_Counter.getValue()
    elseif curPlayer == "Four" then
        playerHand = Player.Blue.getHandObjects(1)
        handSize = Player_Four_Hand_Counter.getValue()
    end
    --print(playerHand[1].getName())


    local amntInHand = 0

    for a,b in pairs(playerHand) do
        local searchHand = b.getName()

        if searchHand == "Topping" then
            amntInHand = amntInHand + 1
        elseif searchHand == "Premium" then
            amntInHand = amntInHand + 1
        elseif searchHand == "Special Sauce" then
            amntInHand = amntInHand + 1
        end
    end
    if curPlayer == "One" then
        if deck ~= nil then
            local amntInDeck = deck.getQuantity()
            --print(amntInDeck)
            if amntInDeck >= 1 then
                deck.dealToColor(handSize - amntInHand, "White")
            elseif amntInDeck == -1 and amntInHand < 5 then
                deck.dealToColor(1, "White")
            end
        end
    elseif curPlayer == "Two" then
        if deck ~= nil then
            local amntInDeck = deck.getQuantity()
            --print(amntInDeck)
            if amntInDeck >= 1 then
                deck.dealToColor(handSize - amntInHand, "Red")
            elseif amntInDeck == -1 and amntInHand < 5 then
                deck.dealToColor(1, "Red")
            end
        end
    elseif curPlayer == "Three" then
        if deck ~= nil then
            local amntInDeck = deck.getQuantity()
            --print(amntInDeck)
            if amntInDeck >= 1 then
                deck.dealToColor(handSize - amntInHand, "Green")
            elseif amntInDeck == -1 and amntInHand < 5 then
                deck.dealToColor(1, "Green")
            end
        end
    elseif curPlayer == "Four" then
        if deck ~= nil then
            local amntInDeck = deck.getQuantity()
            --print(amntInDeck)
            if amntInDeck >= 1 then
                deck.dealToColor(handSize - amntInHand, "Blue")
            elseif amntInDeck == -1 and amntInHand < 5 then
                deck.dealToColor(1, "Blue")
            end
        end
    end
end



