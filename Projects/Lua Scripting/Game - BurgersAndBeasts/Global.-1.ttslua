--THIS WILL TRACK ANYTHING PICKED UP
function onObjectPickUp(player_color, picked_up_object)
    if picked_up_object.getDescription() == "Beast" then
        beastAlreadyInList = false
        for i, v in ipairs(dead_beasts) do
            if v == picked_up_object.getName() then
                beastAlreadyInList = true
            end
        end
        if beastAlreadyInList == false then
            table.insert(dead_beasts, picked_up_object.getName())
            current_tracker_index = 6
            Beast_Tracker.setPosition(beastTrackerLocations["pos"][current_tracker_index])
            Beast_Tracker.setColorTint({255/255, 66/255, 0/255})

        end
    end
    if picked_up_object.getDescription() == "Rune" then
        fillRuneZone()
    end
    if picked_up_object.getDescription() == "Quest" then
        fillQuestZone()
    end
end

