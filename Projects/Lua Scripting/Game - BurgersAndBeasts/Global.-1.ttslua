function findRace(parms)
    local cardIndex = 1
    local foundCard = false

    name = parms.needed --table that just contains name of char on player board that we are matching

    while cardIndex < 76 and foundCard == false do
        if charDeckTable[cardIndex]["cardname"] == name then
            foundCard = true
        else
            cardIndex = cardIndex + 1
        end
    end

    return charDeckTable[cardIndex]["race"]
end

